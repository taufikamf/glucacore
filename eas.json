{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "developmentClient": true
    },
    "preview4": {
      "distribution": "internal"
    },
    "production": {
      "android": {
        "image": "latest",
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "withoutCredentials": true,
        "autoIncrement": false,
        "env": {
          "GRADLE_OPTS": "-Dorg.gradle.project.android.disableAutomaticComponentCreation=true"
        }
      },
      "ios": {
        "image": "latest"
      }
    }
  },
  "cli": {
    "version": ">= 0.34.0",
    "requireCommit": true,
    "appVersionSource": "remote"
  }
}